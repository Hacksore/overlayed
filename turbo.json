{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "tasks": {
    "build": {
      "dependsOn": ["gen","^build"],
      "outputs": ["dist/**", ".vercel/output/**"]
    },
    "gen" :   {
      "dependsOn": ["^gen"],
      "outputs": ["./apps/web/licenses.json"]
    },




    "lint": {},
    "clean": {
      "cache": false
    },
    "format": {},
    "check:format": {},
    "start": {
      "cache": false,
      "persistent": true
    }
  }
}





